{
  "name": "acr-image-promotion-action",
  "version": "1.0.0",
  "description": "GitHub Action for promoting container images between Azure Container Registry environments",
  "main": "dist/index.js",
  "scripts": {
    "build": "ncc build src/main.ts -o dist --source-map --license licenses.txt",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "package": "npm run build && git add dist/",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DigitalInnovation/acr-image-promotion-action.git"
  },
  "keywords": [
    "azure",
    "container-registry",
    "docker",
    "image-promotion",
    "github-actions"
  ],
  "author": "BrightCloud Platform Team",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@azure/arm-containerregistry": "^10.1.0",
    "@azure/identity": "^4.0.1",
    "@azure/core-auth": "^1.5.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.2"
  }
}